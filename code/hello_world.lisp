(defun make-comparisons-list (fields)
	   (loop while fields
		collecting (make-comparison-expr (pop fields) (pop fields))))
(defmacro where (&rest clauses)
  ` #'(lambda (cd) (and ,@(make-comparisons-list clauses))))

